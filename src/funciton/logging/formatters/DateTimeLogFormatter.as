package funciton.logging.formatters {		import funciton.logging.Logging;	import funciton.models.LogMessage;	import funciton.utils.NumberUtils;		public function DateTimeLogFormatter(logMessage:LogMessage):String{		var d:Date = logMessage.date;		var dateTime:String = [d.fullYear, NumberUtils.leftFillWithChar(d.month + 1, "0", 2), NumberUtils.leftFillWithChar(d.date, "0", 2)].join("/");		dateTime += " " + [NumberUtils.leftFillWithChar(d.hours, "0", 2), NumberUtils.leftFillWithChar(d.minutes, "0", 2), NumberUtils.leftFillWithChar(d.seconds, "0", 2)].join(":");		var type:String = Logging.getFlagLabel(logMessage.type);				return [dateTime, "[" + type + "]", logMessage.environment, logMessage.message].join(" ");	}	}