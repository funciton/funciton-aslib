package funciton.models {		public class LogMessage {				private var $__type:uint;		private var $__environment:String;		private var $__message:String;		private var $__date:Date;				public function LogMessage(type:uint, msg:String, env:String = "default"){			$__type = type;			$__environment = env;			$__message = msg;			$__date = new Date();		}				public function get type():uint{ return $__type; }				public function get environment():String{ return $__environment; }				public function get message():String{ return $__message; }				public function get date():Date{ return $__date; }			}	}